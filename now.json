{
  "static": {
  "directoryListing": false,
  "redirects": [
    { "source": "/", "destination": "/ExportAll.trex" },
    { "source": "/download", "destination": "/ExportAll.trex" }
  ],
  "headers": [
    {
      "source" : "**/*.@(trex)",
      "headers" : [{
        "key" : "Content-Type",
        "value" : "application/octet-stream"
      }]
    },{
      "source" : "**/*.@(trex)",
      "headers" : [{
        "key" : "Content-Description",
        "value" : "File Transfer"
      }]
    },{
      "source" : "**/*.@(trex)",
      "headers" : [{
        "key" : "Content-Disposition",
        "value" : "attachment; filename=ExportAll.trex"
      }]
    }]
  }
}
